description=\"`Pencarian '${search}' tidak tersedia. Silahkan cari kata kunci yang sesuai`\"\n      >\n      </md-table-empty-state>\n\n      <md-table-row slot=\"md-table-row\" slot-scope=\"{ item }\">\n        <md-table-cell md-label=\"Gambar\"\n          ><img\n            :src=\"require(`@/assets/img/articles/${item.img}`)\"\n            class=\"img-raised\"\n            style=\"\n              object-fit: cover;\n              width: 100%;\n              height: 15vh;\n              max-width: 15vh;\n            \"\n        /></md-table-cell>\n        <md-table-cell md-label=\"Judul\"\n          ><a :href=\"`#/article/${item.link}`\" style=\"float: left\">{{\n            item.title\n          }}</a\n          ><br /><small style=\"float: left\">{{ item.highlight }}..</small\n          ><br /><br /><small style=\"float: right\">{{\n            moment(item.date).fromNow()\n          }}</small></md-table-cell\n        >\n        <md-table-cell md-label=\"Link\"\n          ><small\n            ><md-button\n              class=\"md-simple just-icon md-warning md-sm\"\n              :href=\"`#/article/${item.link}`\"\n              ><md-icon>read_more</md-icon></md-button\n            ></small\n          ></md-table-cell\n        >\n      </md-table-row>\n    </md-table>\n    <div class=\"md-layout md-gutter md-alignment-top-center\">\n      <pagination v-model=\"infoPagination\" :page-count=\"3\"> </pagination>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Pagination } from \"@/components\";\nimport sourceArticle from \"@/static/article.json\";\n\nimport moment from \"moment\";\n\nconst toLower = (text) => {\n  return text.toString().toLowerCase();\n};\n\nconst searchByName = (items, term) => {\n  if (term) {\n    return items.filter((item) => toLower(item.title).includes(toLower(term)));\n  }\n\n  return items;\n};\n\nexport default {\n  name: \"simple-table\",\n  components: {\n    Pagination,\n  },\n  props: {\n    tableHeaderColor: {\n      type: String,\n      default: \"\",\n    },\n  },\n  data() {\n    return {\n      moment: moment,\n      search: null,\n      articles: [],\n      infoPagination: 1,\n      LAZarticle: sourceArticle.laz,\n      LNWarticle: sourceArticle.lnw,\n    };\n  },\n  methods: {\n    searchOnTable() {\n      this.articles[0] = searchByName(this.LAZarticle, this.search);\n      this.articles[1] = searchByName(this.LNWarticle, this.search);\n    },\n  },\n  created() {\n    this.articles = [this.LAZarticle, this.LNWarticle];\n  },\n};\n</script>\n"]}]}